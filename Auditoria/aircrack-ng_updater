#!/bin/bash

# Script by www.seguridadwireless.net

SCRIPT_VERSION=20180408

############################################################
## Funciones comunes. Su nombre empieza por f_ ##
############################################################
f_variables(){
	. /opt/wifislax-updaters/Funciones/funciones_updater
}

############################################################
## Funciones propias del script. Su nombre empieza por F_ ##
############################################################
# Definicion de variables
F_variables() {
	PRGNAM=aircrack-ng
	echo -ne "\033]2;${PRGNAM}_updater\007"
	WEB=https://github.com/${PRGNAM}/${PRGNAM}
	VERSION=`echo $(curl -s https://raw.githubusercontent.com/${PRGNAM}/${PRGNAM}/master/VERSION | sed "s/-/_/g")_$(curl -s $WEB|grep commit/|head -1|cut -d / -f5|cut -c1-7)`
}

# Verificamos
F_download() {
		echo ""
		echo "$CYAN"Descargando sources de $PRGNAM-$VERSION"$CIERRE"
		sleep 1
		echo ""
		git clone $WEB
}

# Compilamos
F_compila() {
	cd $TMP/$PRGNAM
	echo $CYAN
	echo "Compilamos las sources de$VERDE $PRGNAM-$VERSION"
	sleep 1
        export LDFLAGS="-L/usr/lib${LIBDIRSUFFIX}"
	./autogen.sh
	make experimental=true prefix=/usr sqlite=true libnl=true pcre=true 
	sleep 1
	make experimental=true prefix=/usr sqlite=true libnl=true pcre=true install
# Actualizamos la oui.list
	echo $VERDE
	echo "Actualizando aircrack oui.list ...."
	echo $AMARILLO
	airodump-ng-oui-update
	sleep 1
# Instalamos
	make experimental=true prefix=/usr sqlite=true libnl=true pcre=true DESTDIR=$PKG install &>/dev/null
	mkdir -p $PKG/etc/aircrack-ng/ &>/dev/null
	cp /etc/aircrack-ng/airodump-ng-oui.txt $PKG/etc/aircrack-ng/airodump-ng-oui.txt &>/dev/null

# Meto una version clasica de airmon-ng
mv -f $PKG/usr/sbin/airmon-ng $PKG/usr/sbin/airmon-zc

echo "
